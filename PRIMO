def primo(number):
    if number >= 1:
        for i in range(1, number):
            if number % i != 0:
                print(number, 'TRUE ')
            else:
                print(number, 'FALSE')
            break
        return
print("\n\t\t --- PRIMO --- \n")
n =int(input("DIGITE SEU NUMERO :"))
result = primo(n)
print(f"{result}")
